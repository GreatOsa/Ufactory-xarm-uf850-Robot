<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="uf850">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import uf850 urdf file -->
    <xacro:include filename="$(find uf850_description)/urdf/uf850.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="uf850.ros2_control.xacro" />


    <xacro:uf850_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
