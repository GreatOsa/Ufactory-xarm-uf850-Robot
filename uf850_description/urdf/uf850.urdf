<robot name="uf850">
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link_base.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <link name="link1">
    <visual>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link1.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_link1" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
     <limit lower="0.0" upper="0.3" effort="10" velocity="0.5"/>
  </joint>

  <link name="link2">
    <visual>
      <origin xyz="0 0 0" rpy="0 -1.57 1.57"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link2.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link1_to_link2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/> 
     <axis xyz="0 1 0"/> 
    <limit lower="-1.57"
           upper="1.57"
           effort="10.0"
           velocity="1.0"/>
  </joint>

  <link name="link3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.57"/> 
      <geometry>
        <mesh filename="package://uf850_description/meshes/link3.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link2_to_link3" type="revolute">
   <axis xyz="0 0 1"/>
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0.4" rpy="1.57 1.57 3.14"/> 
      <limit lower="-4.71"
           upper="0"
           effort="10.0"
           velocity="1.0"/>
  </joint>

  <link name="link4">
    <visual>
      <origin xyz="0.3 0 0" rpy="1.57 -1.57 1.57"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link4.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link3_to_link4" type="continuous">
    <axis xyz="1 0 0"/>
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.1 -0.15 0" rpy="1.57 0 0"/> //
  </joint>

  <link name="link5">
    <visual>
      <origin xyz="0 0.1 0" rpy="1.57 1.57 3.14"/> 
      <geometry>
        <mesh filename="package://uf850_description/meshes/link5.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link4_to_link5" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0.3 0.1 0" rpy="3.14 0 0"/> 
  </joint>

  <link name="link6">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <mesh filename="package://uf850_description/meshes/link6.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link5_to_link6" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.1 0.1 0" rpy="0 1.57 0"/> 
  </joint>

<link name="camera_link">
  <visual>
    <geometry>
      <box size="0.05 0.05 0.05"/>
    </geometry>
    <material name="blue">
      <color rgba="0.1 0.1 0.8 1"/>
    </material>
  </visual>
  <inertial>    
    <mass value="0.05"/>
    <inertia ixx="1e-4" ixy="0" ixz="0" iyy="1e-4" iyz="0" izz="1e-4"/>
  </inertial>
</link>

<joint name="camera_mount_joint" type="fixed">
  <parent link="link6"/>
  <child link="camera_link"/>
  <origin xyz="0 0 0" rpy="0 0 0"/>
</joint>
</robot>
